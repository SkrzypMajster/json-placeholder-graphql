{
  "name": "json-placeholder-graphql",
  "version": "1.0.0",
  "description": "GraphQL API with TypeScript based on collections from JSON Placeholder REST API",
  "type": "module",
  "exports": "./index.js",
  "engines": {
    "node": "^12.20.0 || ^14.13.1 || >=16.0.0"
  },
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon",
    "lint": "eslint . --ext .ts",
    "lint:fix": "npm run lint -- --fix",
    "format": "prettier --write '**/*.ts'",
    "build": "tsc",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/SkrzypMajster/json-placeholder-graphql.git"
  },
  "keywords": [],
  "author": "Dawid 'SkrzypMajster' Skrzypczyk",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/SkrzypMajster/json-placeholder-graphql/issues"
  },
  "homepage": "https://github.com/SkrzypMajster/json-placeholder-graphql#readme",
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "devDependencies": {
    "@types/express": "4.17.13",
    "@types/jest": "27.4.0",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "eslint": "7.32.0",
    "eslint-config-standard": "16.0.3",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "5.2.0",
    "husky": "7.0.4",
    "jest": "27.4.7",
    "nodemon": "2.0.15",
    "prettier": "2.5.1",
    "pretty-quick": "3.1.3",
    "ts-jest": "27.1.3",
    "ts-node": "10.4.0",
    "typescript": "4.5.5"
  },
  "dependencies": {
    "dotenv": "16.0.0",
    "express": "4.17.2",
    "express-graphql": "0.12.0",
    "graphql": "16.3.0",
    "lowdb": "3.0.0",
    "perron": "0.11.4"
  }
}
